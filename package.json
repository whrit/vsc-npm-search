{
  "name": "npm-package-search",
  "displayName": "NPM Package Search",
  "description": "Search npm packages and get version info directly in VS Code",
  "icon": "icon-vsce-npm.png",
  "version": "1.2.3",
  "publisher": "Quikturn",
  "repository": {
    "type": "git",
    "url": "git://github.com/whrit/vsc-npm-search.git"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "npmSearch.searchPackage",
        "title": "NPM: Search Package"
      },
      {
        "command": "npmSearch.searchWithQualifiers",
        "title": "NPM: Advanced Search (with qualifiers)"
      },
      {
        "command": "npmSearch.getLatestVersion",
        "title": "NPM: Get Latest Version"
      },
      {
        "command": "npmSearch.getInstallCommand",
        "title": "NPM: Get Install Command"
      },
      {
        "command": "npmSearch.searchSuggestions",
        "title": "NPM: Search Suggestions"
      },
      {
        "command": "npmSearch.viewVersionHistory",
        "title": "NPM: View Package Version History"
      },
      {
        "command": "npmSearch.searchSelectedText",
        "title": "NPM: Search Selected Text"
      },
      {
        "command": "npmSearch.searchMultiplePackages",
        "title": "NPM: Search Multiple Selected Packages"
      },
      {
        "command": "npmSearch.analyzePackageJson",
        "title": "NPM: Analyze Package.json"
      },
      {
        "command": "npmSearch.searchFromClipboard",
        "title": "NPM: Search from Clipboard"
      },
      {
        "command": "npmSearch.updateAllPackages",
        "title": "NPM: Update All Packages to Latest Versions"
      }
    ],
    "keybindings": [
      {
        "command": "npmSearch.searchPackage",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      },
      {
        "command": "npmSearch.searchSelectedText",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h"
      },
      {
        "command": "npmSearch.updateAllPackages",
        "key": "ctrl+option+a",
        "mac": "cmd+option+a",
        "when": "resourceExtname == .json && resourceFilename == package.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "npmSearch.searchSelectedText",
          "group": "npm-search",
          "when": "editorHasSelection"
        },
        {
          "command": "npmSearch.searchMultiplePackages",
          "group": "npm-search",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run lint && pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "deploy": "vsce publish --no-dependencies",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "keywords": [
    "npm",
    "package",
    "search",
    "registry",
    "dependencies",
    "version",
    "package.json"
  ],
  "author": "Beckett Whritenour",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.15",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "@vscode/vsce": "^3.0.0"
  }
}
