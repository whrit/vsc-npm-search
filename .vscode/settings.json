{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "out": false,
    "node_modules": true,
    ".vscode-test": true
  },
  "search.exclude": {
    "out": true,
    "node_modules": true,
    ".vscode-test": true
  }
}